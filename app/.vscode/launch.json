    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug: Clean the Planet",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "Debug: Clean the Planet (Production Env.)",
            "request": "launch",
            "type": "dart",
            "args": [
                "--dart-define",
                "API_URL=https://clean-the-planet.herokuapp.com"
            ],
        },
        {
            "name": "Release: Clean the Planet (Release Mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Test: Clean the Planet",
            "request": "launch",
            "type": "dart",
            "program": "./test/"
        },
    ]
}